<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="studentTestingService" class="ru.kotikov.services.StudentTestingService">
        <constructor-arg index="0" ref="iOService"/>
        <constructor-arg index="1" ref="resultPrinter"/>
        <constructor-arg index="2" ref="questionDao"/>
    </bean>
    <bean id="iOService" class="ru.kotikov.services.AnswerService"/>
    <bean id="questionDao" class="ru.kotikov.dao.QuestionDao">
        <constructor-arg index="0" value="questions.csv"/>
    </bean>
    <bean id="resultPrinter" class="ru.kotikov.services.ResultPrinterImpl"/>

</beans>
